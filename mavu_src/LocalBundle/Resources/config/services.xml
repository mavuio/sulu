<?xml version="1.0" encoding="UTF-8"?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd"  xmlns:doctrine="http://symfony.com/schema/dic/doctrine">
    
    <parameters>
        <parameter key="env(NPM_BINARY)">npm</parameter>
    </parameters>
    
    <services>
        
        <defaults autowire="true" autoconfigure="true" public="false">
            <bind key="$projectDir">%kernel.project_dir%</bind> 
        </defaults>

        <service id="Mavu\LocalBundle\Core\LocalHelpers" class="Mavu\LocalBundle\Core\LocalHelpers">
            <argument type="service" id="sulu_document_manager.document_manager" />
        </service>  

        <service id="Sulu\Bundle\MarkupBundle\Markup\MarkupParserInterface" alias="sulu_markup.parser" public="false" />



    </services>
</container>
