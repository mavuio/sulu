<?xml version="1.0" ?>
<list xmlns="http://schemas.sulu.io/list-builder/list">
    <key>pages</key>

    <properties>
        <property
            name="id"
            sortable="false"
            translation="sulu_admin.id"
            visibility="no"
        />
        <property
            name="title"
            sortable="false"
            translation="sulu_admin.title"
            visibility="always"
        />
        <property
        name="url"
        sortable="false"
        translation="url"
        visibility="yes"
        />
        <property
        name="template"
        sortable="false"
        translation="template"
        visibility="yes"
        />
        <property
            name="published"
            sortable="false"
            translation="sulu_admin.published"
            visibility="yes"
        >
            <transformer type="datetime"/>
        </property>
    </properties>
</list>
