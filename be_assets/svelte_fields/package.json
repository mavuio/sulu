{
  "name": "svelte_fields",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "codearea_watch": "pnpm codearea_clean; VITE_MODE=prod vite build --watch -c ./src/codearea/vite_codearea.config.js",
    "codearea_build": "pnpm codearea_clean; VITE_MODE=prod vite build -c ./src/codearea/vite_codearea.config.js",
    "codearea_clean": "rm -f ../../public/be_assets/codearea*",
    "tabbar_watch": "pnpm tabbar_clean; VITE_MODE=prod vite build --watch -c ./src/tabbar/vite_tabbar.config.js",
    "tabbar_build": "pnpm tabbar_clean; VITE_MODE=prod vite build -c ./src/tabbar/vite_tabbar.config.js",
    "tabbar_clean": "rm -f ../../public/be_assets/tabbar*",
    "teaserdata_preview_watch": "pnpm teaserdata_preview_clean; VITE_MODE=prod vite build --watch -c ./src/teaserdata_preview/vite_teaserdata_preview.config.js",
    "teaserdata_preview_build": "pnpm teaserdata_preview_clean; VITE_MODE=prod vite build -c ./src/teaserdata_preview/vite_teaserdata_preview.config.js",
    "teaserdata_preview_clean": "rm -f ../../public/be_assets/teaserdata_preview*",
    "build_all": "pnpm tabbar_build; pnpm codearea_build; pnpm teaserdata_preview_build;"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.2.0",
    "@tsconfig/svelte": "^4.0.1",
    "autoprefixer": "10.4.14",
    "postcss": "^8.4.23",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-load-config": "^4.0.1",
    "postcss-variables": "^1.1.1",
    "svelte": "^3.59.1",
    "vite": "^4.3.5"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.38.0",
    "monaco-tailwindcss": "^0.6.0",
    "postcss-nested": "^6.0.1",
    "svelte-inspect": "^0.1.3",
    "svelte-preprocess": "^5.0.3",
    "tailwindcss": "^3.3.2"
  }
}
