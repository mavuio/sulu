{
  "name": "svelte_widgets",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "terminbox_live": "vite serve -c ./src/terminbox/vite_config_terminbox.js",
    "terminbox_watch": "pnpm terminbox_clean; VITE_MODE=prod vite build --watch -c ./src/terminbox/vite_config_terminbox.js",
    "terminbox_build": "pnpm terminbox_clean; VITE_MODE=prod vite build -c ./src/terminbox/vite_config_terminbox.js",
    "terminbox_clean": "rm -f ../../public/fe_assets/terminbox*",
    "eventsuche_live": "vite serve -c ./src/eventsuche/vite_config_eventsuche.js",
    "eventsuche_watch": "pnpm eventsuche_clean; VITE_MODE=prod vite build --watch -c ./src/eventsuche/vite_config_eventsuche.js",
    "eventsuche_build": "pnpm eventsuche_clean; VITE_MODE=prod vite build -c ./src/eventsuche/vite_config_eventsuche.js",
    "eventsuche_clean": "rm -f ../../public/fe_assets/eventsuche*",
    "ticker_live": "vite serve -c ./src/ticker/vite_config_ticker.js",
    "ticker_watch": "pnpm ticker_clean; VITE_MODE=prod vite build --watch -c ./src/ticker/vite_config_ticker.js",
    "ticker_build": "pnpm ticker_clean; VITE_MODE=prod vite build -c ./src/ticker/vite_config_ticker.js",
    "ticker_clean": "rm -f ../../public/fe_assets/ticker*"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^3.0.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "svelte": "^3.44.0",
    "tailwindcss": "^3.0.19",
    "vite": "^2.8.0"
  },
  "dependencies": {
    "@beyonk/svelte-datepicker": "^12.0.1",
    "@tailwindcss/forms": "^0.5.1",
    "dayjs": "^1.11.2",
    "es6-object-assign": "^1.1.0",
    "lodash-es": "^4.17.21",
    "postcss-load-config": "^3.1.3",
    "postcss-nested": "^5.0.6",
    "svelte-preprocess": "^4.10.4"
  }
}
